{
  "name": "tesla-dashboard-server",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "postinstall": "npm link tesla-dashboard-api",
    "dev": "tsc-watch --onSuccess \"node -r ./env ./dist/server.js\"",
    "clean": "rimraf dist",
    "deploy": "docker build -t tesla-dashboard-server .",
    "poll": "npm run build && node -r ./env ./dist/sync.js jchull",
    "build": "tsc",
    "start": "node -r ./env ./dist/server.js",
    "lint": "eslint './src/**/*.{ts,tsx}'"
  },
  "dependencies": {
    "axios": "^0.19.0",
    "bcrypt": "^3.0.7",
    "body-parser": "^1.19.0",
    "compression": "^1.7.4",
    "cookie-parser": "^1.4.4",
    "cross-env": "^6.0.3",
    "debug": "^4.1.1",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "http-status-codes": "^1.4.0",
    "jsonwebtoken": "^8.5.1",
    "module-alias": "^2.2.2",
    "mongodb": "^3.4.1",
    "mongoose": "^5.8.2",
    "morgan": "^1.9.1"
  },
  "jest": {
    "verbose": true,
    "transform": {
      "^.+\\.tsx?$": "ts-jest"
    },
    "testRegex": "(/test/.*|(\\.|/)(test|spec))\\.(js|ts)x?$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ]
  },
  "devDependencies": {
    "@types/axios": "^0.14.0",
    "@types/bcrypt": "^3.0.0",
    "@types/compression": "^1.0.1",
    "@types/cookie-parser": "^1.4.2",
    "@types/express": "^4.17.2",
    "@types/find": "^0.2.1",
    "@types/jest": "^24.0.24",
    "@types/jsonfile": "^5.0.0",
    "@types/jsonwebtoken": "^8.3.5",
    "@types/mongodb": "^3.3.13",
    "@types/mongoose": "^5.5.34",
    "@types/morgan": "^1.7.37",
    "@types/node": "^12.12.21",
    "@types/randomstring": "^1.1.6",
    "@typescript-eslint/eslint-plugin": "^2.12.0",
    "@typescript-eslint/parser": "^2.12.0",
    "babel-eslint": "^10.0.3",
    "eslint": "^6.8.0",
    "find": "^0.3.0",
    "fs-extra": "^8.1.0",
    "jest": "^24.9.0",
    "jsonfile": "^5.0.0",
    "pretty": "^2.0.0",
    "rimraf": "^3.0.0",
    "ts-jest": "^24.2.0",
    "ts-node": "^8.5.4",
    "tsconfig-paths": "^3.9.0",
    "typescript": "^3.7.4"
  }
}
