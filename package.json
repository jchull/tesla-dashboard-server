{
  "name": "tesla-dashboard-server",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "postinstall": "npm link tesla-dashboard-api",
    "dev": "tsc-watch --onSuccess \"node ./dist/server.js\"",
    "clean": "rimraf dist",
    "deploy": "docker build -t tesla-dashboard-server .",
    "poll": "npm run build && node ./dist/sync.js jchull",
    "build": "tsc",
    "start": "cross-env ENV_FILE=production node -r ./env -r module-alias/register ./dist/start.js"
  },
  "dependencies": {
    "axios": "^0.19.0",
    "body-parser": "^1.19.0",
    "compression": "^1.7.4",
    "debug": "^4.1.1",
    "mongodb": "^3.3.2",
    "mongoose": "^5.6.8",
    "bcrypt": "^3.0.6",
    "cookie-parser": "^1.4.4",
    "cross-env": "^5.2.0",
    "dotenv": "^8.1.0",
    "express": "^4.17.1",
    "http-status-codes": "^1.3.2",
    "jsonwebtoken": "^8.5.1",
    "module-alias": "^2.2.1",
    "morgan": "^1.9.1"
  },
  "jest": {
    "verbose": true,
    "transform": {
      "^.+\\.tsx?$": "ts-jest"
    },
    "testRegex": "(/test/.*|(\\.|/)(test|spec))\\.(js|ts)x?$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ]
  },
  "devDependencies": {
    "@types/axios": "^0.14.0",
    "@types/bcrypt": "^3.0.0",
    "@types/compression": "0.0.36",
    "@types/cookie-parser": "^1.4.2",
    "@types/dotenv": "^6.1.1",
    "@types/express": "^4.17.1",
    "@types/find": "^0.2.1",
    "@types/jest": "^24.0.16",
    "@types/jsonfile": "^5.0.0",
    "@types/jsonwebtoken": "^8.3.3",
    "@types/mongodb": "^3.1.31",
    "@types/mongoose": "^5.5.11",
    "@types/morgan": "^1.7.37",
    "@types/node": "^12.7.2",
    "@types/randomstring": "^1.1.6",
    "eslint": "^6.3.0",
    "find": "^0.3.0",
    "fs-extra": "^8.1.0",
    "jest": "^24.8.0",
    "jsonfile": "^5.0.0",
    "pretty": "^2.0.0",
    "rimraf": "^2.6.3",
    "ts-jest": "^24.0.2",
    "ts-node": "^8.3.0",
    "tsconfig-paths": "^3.8.0",
    "typescript": "^3.5.3"
  }
}
